import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner scan=new Scanner (System.in);
        int cost1=scan.nextInt();
        int quan1=scan.nextInt();
        int cost2=scan.nextInt();
        int quan2=scan.nextInt();
        int cost3=scan.nextInt();
        int quan3=scan.nextInt();
        int budget=scan.nextInt();
        double totalbeforetax=cost1*quan1+cost2*quan2+cost3*quan3;
        double totalaftertax=totalbeforetax*1.10;
        System.out.printf("Total (Before Tax): %.2f\n",totalbeforetax);
        System.out.printf("Total (After Tax): %.2f\n",totalaftertax);
        if (totalaftertax <= budget) {
            System.out.println("Within Budget");
        }
        else
            System.out.println("Over Budget");
    
    }
}
